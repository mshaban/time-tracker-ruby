<h1>Projects#index</h1>
<p>Find me in app/views/projects/index.html.erb</p>

<table>
  <tbody>
    <tr>
      <th>Project</th>
      <th>Company</th>
    </tr>
    <% @projects.each do |project| %>
        <tr>
          <td><%= link_to project, project %></td>
          <td><%= link_to project.company.name, project.company %></td>
        </tr>
    <% end %>
  </tbody>
</table>

<% content_for :aside do %>
    <% render 'sidebar' %>
<% end %>